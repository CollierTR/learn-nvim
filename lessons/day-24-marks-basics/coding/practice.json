    "section_f_deployment_config": {
      "kubernetes_clusters": [
        {
          "name": "production",
          "region": "us-west-2",
          "version": "1.28.0",
          "node_groups": [
            {
              "name": "control-plane",
              "instance_type": "t3.medium",
              "min_size": 3,
              "max_size": 3,
              "desired_capacity": 3
            },
            {
              "name": "worker-nodes",
              "instance_type": "t3.large",
              "min_size": 5,
              "max_size": 20,
              "desired_capacity": 10
            }
          ]
        },
        {
          "name": "staging",
          "region": "us-west-2",
          "version": "1.28.0",
          "node_groups": [
            {
              "name": "control-plane",
              "instance_type": "t3.small",
              "min_size": 1,
              "max_size": 1,
              "desired_capacity": 1
            },
            {
              "name": "worker-nodes",
              "instance_type": "t3.medium",
              "min_size": 2,
              "max_size": 5,
              "desired_capacity": 3
            }
          ]
        }
      ]
    },
    "section_g_backup_configuration": {
      "database_backups": {
        "schedule": "0 2 * * *",
        "retention_days": 30,
        "compression": true,
        "encryption": true,
        "storage": {
          "type": "s3",
          "bucket": "company-database-backups",
          "region": "us-west-2",
          "storage_class": "STANDARD_IA"
        }
      },
      "application_backups": {
        "schedule": "0 3 * * *",
        "retention_days": 14,
        "compression": true,
        "storage": {
          "type": "s3",
          "bucket": "company-application-backups",
          "region": "us-west-2"
        }
      }
    },
    "section_h_logging_configuration": {
      "elasticsearch": {
        "cluster_name": "production-logs",
        "nodes": [
          {
            "host": "es-master-1.logging.company.local",
            "port": 9200,
            "roles": ["master", "data"]
          },
          {
            "host": "es-master-2.logging.company.local",
            "port": 9200,
            "roles": ["master", "data"]
          },
          {
            "host": "es-master-3.logging.company.local",
            "port": 9200,
            "roles": ["master", "data"]
          }
        ],
        "index_settings": {
          "number_of_shards": 3,
          "number_of_replicas": 1,
          "refresh_interval": "5s"
        }
      },
      "kibana": {
        "host": "kibana.logging.company.local",
        "port": 5601,
        "elasticsearch_hosts": [
          "http://es-master-1.logging.company.local:9200",
          "http://es-master-2.logging.company.local:9200",
          "http://es-master-3.logging.company.local:9200"
